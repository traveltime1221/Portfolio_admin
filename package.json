{
  "name": "project_test_v2",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predeploy": "npm run build && cp dist/index.html dist/404.html && touch dist/.nojekyll",
    "deploy": "gh-pages -d dist -r https://github.com/traveltime1221/Portfolio_admin.git -b gh-pages",
    "build_prod": "vite build --mode production --outDir dist_prod",
    "build_dev": "vite build --mode development --outDir dist_dev",
    "preview_prod": "vite preview --outDir dist_prod",
    "preview_dev": "vite preview --outDir dist_dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "analyze": "vite build --analyze",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@fullcalendar/vue3": "^6.1.17",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.14",
    "@types/leaflet": "^1.9.18",
    "apexcharts": "^4.5.0",
    "dropzone": "^6.0.0-beta.2",
    "echarts": "^5.6.0",
    "flatpickr": "^4.6.13",
    "i": "^0.3.7",
    "jsvectormap": "^1.6.0",
    "leaflet": "^1.9.4",
    "npm": "^11.4.2",
    "pinia": "^3.0.1",
    "swiper": "^11.2.1",
    "vue": "^3.5.13",
    "vue-flatpickr-component": "^12.0.0",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0",
    "vue3-draggable-resizable": "^1.6.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.15",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-oxlint": "^0.15.13",
    "eslint-plugin-vue": "~10.0.0",
    "gh-pages": "^6.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.13",
    "postcss": "^8.5.1",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.83.4",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.8.0",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.1",
    "vite-plugin-pages": "^0.33.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
